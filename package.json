{
  "name": "aleksandrow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "codegen": "graphql-codegen --config codegen.ts",
    "build:release": "npm-run-all build mkdir copy",
    "mkdir": "cross-env mkdir -p ./build/.next ./build/public/_next/static/",
    "copy": "npm-run-all copy:init copy:json copy:public copy:static",
    "copy:init": "cross-env cp -r .next/standalone/. build/.",
    "copy:json": "cross-env cp package.json build",
    "copy:public": "cross-env cp -r public/. build/public/.",
    "copy:static": "cross-env cp -r .next/static/. build/public/_next/static/",
    "start:release": "cd build && node server.js",
    "start": "cross-env HOSTNAME=0.0.0.0 next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "^4.0.11",
    "graphql": "^16.12.0",
    "html-react-parser": "^5.2.11",
    "next": "16.1.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swiper": "^5.4.3",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
